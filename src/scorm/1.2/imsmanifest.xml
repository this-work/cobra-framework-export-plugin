<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<manifest identifier="com.scorm.<%= organization %>.sco"
		  version="1"
		  xmlns="http://www.imsproject.org/xsd/imscp_rootv1p1p2"
		  xmlns:adlcp="http://www.adlnet.org/xsd/adlcp_rootv1p2"
		  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xmlns:imsmd="http://www.imsglobal.org/xsd/imsmd_rootv1p2p1"
		  xsi:schemaLocation="http://www.imsproject.org/xsd/imscp_rootv1p1p2 imscp_rootv1p1p2.xsd
                             http://www.imsglobal.org/xsd/imsmd_rootv1p2p1 imsmd_rootv1p2p1.xsd
                             http://www.adlnet.org/xsd/adlcp_rootv1p2 adlcp_rootv1p2.xsd">

  <metadata>
    <schema>ADL SCORM</schema>
    <schemaversion>1.2</schemaversion>
	<imsmd:lom>
		  <imsmd:general>
			  <imsmd:title>
				  <%= title %>
			  </imsmd:title>
			  <% if (description) { %>
			  <imsmd:description>
				  <%= description %>
			  </imsmd:description>
			  <% } %>
			  <% if (keyword) { %>
			  <imsmd:keyword>
				  <%= keyword %>
			  </imsmd:keyword>
			  <% } %>
		  </imsmd:general>
	</imsmd:lom>
  </metadata>
	<organizations default="<%= organization %>">
		<organization identifier="<%= organization %>">
			<title><%= title %></title>
			<item identifier="course-overview" identifierref="course">
				<title><%= title %></title>
			</item>
		</organization>
	</organizations>
	<resources>
		<resource identifier="course" type="webcontent" adlcp:scormtype="sco" href="./index.html">
		</resource>
	</resources>
</manifest>
